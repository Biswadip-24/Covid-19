<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/back"
        android:paddingBottom="25dp">

        <LinearLayout
            android:id="@+id/linearLayout"
            android:layout_width="match_parent"
            android:layout_height="35dp"
            android:layout_marginTop="210dp"
            android:orientation="horizontal"
            android:background="@color/white">

            <TextView
                android:id="@+id/textView19"
                android:layout_width="100dp"
                android:layout_height="20dp"
                android:layout_marginStart="15dp"
                android:layout_gravity="center_vertical"
                android:fontFamily="@font/raleway_medium"
                android:text="Country"
                android:textColor="@color/black"
                android:textStyle="bold" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:paddingEnd="15dp">

                <TextView
                    android:id="@+id/textView20"
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:gravity="end"
                    android:fontFamily="@font/raleway_medium"
                    android:layout_marginTop="8dp"
                    android:text="@string/Confirmed"
                    android:textColor="@color/red"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/textView21"
                    android:layout_width="0dp"
                    android:layout_height="20dp"
                    android:layout_weight="1"
                    android:fontFamily="@font/raleway_medium"
                    android:layout_marginTop="8dp"
                    android:gravity="end"
                    android:text="@string/recovered"
                    android:textColor="@color/green"
                    android:textStyle="bold" />


                <TextView
                    android:id="@+id/textView18"
                    android:layout_width="0dp"
                    android:layout_height="20dp"
                    android:layout_weight="1"
                    android:layout_marginTop="8dp"
                    android:fontFamily="@font/raleway_medium"
                    android:gravity="end"
                    android:text="@string/Deceased"
                    android:textColor="@color/black"
                    android:textStyle="bold" />

            </LinearLayout>
        </LinearLayout>



        <View
            android:id="@+id/view3"
            android:layout_width="wrap_content"
            android:layout_height="120dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginEnd="10dp"
            android:layout_marginStart="10dp"
            android:layout_marginTop="50dp"
            android:background="@drawable/rounded"
            android:elevation="7dp" />

        <TextView
            android:id="@+id/confirm2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/con2"
            android:layout_alignTop="@+id/decease2"
            android:elevation="7dp"
            android:fontFamily="@font/raleway_medium"
            android:text="@string/Confirmed"
            android:textColor="@color/black"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/recover2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="90dp"
            android:elevation="7dp"
            android:fontFamily="@font/raleway_medium"
            android:text="@string/recovered"
            android:textColor="@color/black"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/con2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignTop="@+id/rec2"
            android:layout_marginStart="51dp"
            android:elevation="7dp"
            android:fontFamily="@font/roboto"
            android:text="00000"
            android:textColor="@color/red"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/drec2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/rec2"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="5dp"
            android:elevation="7dp"
            android:fontFamily="@font/roboto"
            android:text="+0"
            android:textAlignment="center"
            android:textColor="@color/green"
            android:textSize="11sp" />

        <TextView
            android:id="@+id/ddec2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/dec2"
            android:layout_below="@+id/rec2"
            android:layout_marginTop="5dp"
            android:elevation="7dp"
            android:fontFamily="@font/roboto"
            android:text="+0"
            android:textAlignment="viewEnd"
            android:textColor="@color/black"
            android:textSize="11sp" />

        <TextView
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginStart="20dp"
            android:layout_marginTop="14dp"
            android:fontFamily="@font/raleway"
            android:text="@string/note"
            android:textColor="@color/white" />

        <TextView
            android:id="@+id/dcon2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/confirm2"
            android:layout_below="@+id/rec2"
            android:layout_marginTop="5dp"
            android:elevation="7dp"
            android:fontFamily="@font/roboto"
            android:text="+0"
            android:textAlignment="viewStart"
            android:textColor="@color/red"
            android:textSize="11sp" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_alignStart="@+id/view3"
            android:layout_marginTop="180dp"
            android:fontFamily="@font/raleway"
            android:text="@string/all_World"
            android:textColor="@color/black"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/decease2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignStart="@+id/dec2"
            android:layout_alignTop="@+id/recover2"
            android:layout_marginStart="0dp"
            android:layout_marginTop="-1dp"
            android:elevation="7dp"
            android:fontFamily="@font/raleway_medium"
            android:text="@string/Deceased"
            android:textColor="@color/black"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="34dp"
            android:layout_marginTop="62dp"
            android:elevation="7dp"
            android:fontFamily="monospace"
            android:text="@string/world"
            android:textAllCaps="false"
            android:textColor="@color/black"
            android:textSize="15sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/dec2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/view3"
            android:layout_alignTop="@+id/rec2"
            android:layout_marginEnd="35dp"
            android:layout_marginTop="0dp"
            android:elevation="7dp"
            android:fontFamily="@font/roboto"
            android:text="00000"
            android:textColor="@color/black"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/rec2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="110dp"
            android:elevation="7dp"
            android:fontFamily="@font/roboto"
            android:text="00000"
            android:textColor="@color/green"
            android:textStyle="bold" />

        <Switch
            android:id="@+id/switch1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/view3"
            android:layout_below="@+id/view3"
            android:layout_marginTop="8dp"
            android:fontFamily="@font/raleway"
            android:text="Yesterday"
            android:textSize="18sp" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="260dp">

            <android.support.v7.widget.RecyclerView
                android:id="@+id/RecyclerView1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:nestedScrollingEnabled="false"
                android:padding="8dp"
                android:scrollbars="none" />

        </RelativeLayout>



    </RelativeLayout>
</RelativeLayout>